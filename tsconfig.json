{
  "compilerOptions": {
    "moduleResolution": "nodeNext",
    "module": "NodeNext",
    "target": "esnext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "*": ["./.yarn/*"]
    }
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
